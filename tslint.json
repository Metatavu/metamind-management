{
  "extends": "tslint:recommended",
  "defaultSeverity": "warning",
  "rules": {
    "max-line-length": {
      "options": [160]
    },
    "import-sources-order": "any"
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
